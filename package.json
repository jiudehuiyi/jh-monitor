{
  "name": "jh-monitor",
  "version": "0.0.2",
  "description": "",
  "main": "dist/index.min.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "pkg": "bash installAll.sh",
    "exampleStart": "bash exampleStart.sh",
    "exampleLink": " bash link.sh && bash exampleLink.sh",
    "build:dev": "rollup -w -c",
    "build:rollup": "rollup -c",
